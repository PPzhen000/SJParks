{
  "name": "sjparks",
  "version": "1.0.0",
  "description": "An app made to help the City of San Jose communicate with residents a little better. ",
  "main": "Server.js",
  "scripts": {
    "build": "react-scripts build",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "client-install": "yarn install --prefix client",
    "eject": "react-scripts eject",
    "start": "node Server.js",
    "client": "yarn start --prefix client",
    "server": "nodemon Server.js",
    "dev-test": "NODE_ENV=test concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev": "NODE_ENV=development concurrently --kill-others-on-fail \"yarn start\" \"yarn client\"",
    "prod": "NODE_ENV=production concurrently --kill-others-on-fail \"yarn start\" \"yarn client\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.13.2",
    "dotenv": "^6.0.0",
    "dotenv-safe": "^6.0.0",
    "express": "^4.16.3",
    "express-ejs-layouts": "^2.5.0",
    "express-flash": "0.0.2",
    "express-form-data": "^2.0.7",
    "express-request-id": "^1.4.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.4",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "nodemon": "^1.18.8",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "react": "^16.8.1",
    "react-app": "^1.1.2",
    "react-dom": "^16.8.1",
    "react-scripts": "^2.1.3",
    "twilio": "^3.26.1",
    "webpack-dev-server": "^3.1.14"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "engines": {
    "node": "8.10.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irishka2863/SJParks.git"
  },
